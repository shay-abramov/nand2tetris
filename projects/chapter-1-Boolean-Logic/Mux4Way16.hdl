    Or	(a=outcd7,	b=outab7,	out=out[7]);
    Or	(a=outcd8,	b=outab8,	out=out[8]);
    Or	(a=outcd9,	b=outab9,	out=out[9]);
    Or	(a=outcd10,	b=outab10,	out=out[10]);
    Or	(a=outcd11,	b=outab11,	out=out[11]);
    Or	(a=outcd12,	b=outab12,	out=out[12]);
    Or	(a=outcd13,	b=outab13,	out=out[13]);
    Or	(a=outcd14,	b=outab14,	out=out[14]);
    Or	(a=outcd15,	b=outab15,	out=out[15]);
}

    And	(a=d[14],	b=seld,		out=outd14);
    And	(a=d[15],	b=seld,		out=outd15);
    Or	(a=outa0,	b=outb0,	out=outab0);
    Or	(a=outa1,	b=outb1,	out=outab1);
    Or	(a=outa2,	b=outb2,	out=outab2);
